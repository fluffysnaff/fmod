{
  "schemaVersion": 1,
  "id": "fmod",
  "version": "${version}",

  "name": "FMod",
  "description": "Best mod for LiveOverflow server",
  "authors": [
    "fluffysnaff"
  ],
  "contact": {
    "repo": "https://gitlab.com/fluffysnaff/fmod"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.addon.fmod.FMod"
    ]
  },
  "mixins": [
    "fmod.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "90,150,170"
  },
  "accessWidener": "meteor-client.accesswidener",
  "custom": {
    "meteor-client:color": "227,0,0",
    "github:sha": "${gh_hash}"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
